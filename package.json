{
  "name": "fifa-api",
  "version": "1.0.0",
  "description": "Fifa custom Team builder API based on express js with es6 and integrated with mongodb",
  "main": "dist/app.js",
  "scripts": {
    "build": "babel ./src --out-dir ./build",
    "start": "nodemon --exec babel-node src/app.js",
    "lint": "eslint ."
  },
  "xo": {
    "prettier": true
  },
  "renovate": {
    "packageRules": [
      {
        "packagePatterns": [
          "eslint",
          "^@types/"
        ],
        "groupName": "eslint",
        "automerge": true
      }
    ]
  },
  "author": "Saurav M. H",
  "url": "https://github.com/sauravhiremath/fifa-api",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "eslint": "^7.14.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.9",
    "socket.io": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.16",
    "@babel/core": "7.12.16",
    "@babel/node": "7.12.16",
    "@babel/preset-env": "7.12.16",
    "@babel/runtime": "7.12.13",
    "algoliasearch": "4.8.5",
    "babel-eslint": "10.1.0",
    "bcrypt": "5.0.0",
    "consola": "2.15.3",
    "dateformat": "4.5.1",
    "dotenv": "8.2.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-xo": "0.34.0",
    "eslint-plugin-prettier": "3.3.1",
    "nodemon": "2.0.7",
    "prettier": "2.2.1"
  }
}
